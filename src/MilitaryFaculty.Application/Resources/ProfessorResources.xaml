                    xmlns:vm="clr-namespace:MilitaryFaculty.Application.ViewModels"
                    xmlns:views="clr-namespace:MilitaryFaculty.Application.Views">
    <DataTemplate DataType="{x:Type vm:ProfessorHeaderViewModel}">
        <views:ProfessorHeaderView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorConferencesViewModel}">
        <views:ProfessorConferencesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorPublicationsViewModel}">
        <views:ProfessorPublicationsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorBooksViewModel}">
        <views:ProfessorBooksView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorExhibitionsViewModel}">
        <views:ProfessorExhibitionsView />
    </DataTemplate>
</ResourceDictionary>