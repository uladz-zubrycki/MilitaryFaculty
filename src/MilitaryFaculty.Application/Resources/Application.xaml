                    xmlns:vm="clr-namespace:MilitaryFaculty.Application.ViewModels"
                    xmlns:views="clr-namespace:MilitaryFaculty.Application.Views">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Resources/Entity.xaml" />
        <ResourceDictionary Source="/Resources/Cathedra.xaml" />
        <ResourceDictionary Source="/Resources/Professor.xaml" />
        <ResourceDictionary Source="/Resources/FacultyTree.xaml" />
        <ResourceDictionary Source="/Resources/Common.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type vm:StartupViewModel}">
        <views:Startup />
    </DataTemplate>
</ResourceDictionary>