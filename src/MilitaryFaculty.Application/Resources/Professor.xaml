    <DataTemplate DataType="{x:Type vm:ProfessorView+Exhibitions}">
        <views:Exhibitions />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorView+CouncilParticipations}">
        <views:CouncilParticipations />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorView+Dissertations}">
        <views:Dissertations />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorView+InventiveApplications}">
        <views:InventiveApplications />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:ProfessorView+EfficiencyProposals}">
        <views:EfficiencyProposals />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ProfessorView+Researches}">
        <views:Researches />
    </DataTemplate>
</ResourceDictionary>