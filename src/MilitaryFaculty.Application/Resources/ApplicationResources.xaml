<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MilitaryFaculty.Application.ViewModels"
                    xmlns:views="clr-namespace:MilitaryFaculty.Application.Views">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="EntityResources.xaml" />
        <ResourceDictionary Source="CathedraResources.xaml" />
        <ResourceDictionary Source="ProfessorResources.xaml" />
        <ResourceDictionary Source="FacultyTreeResources.xaml" />
        <ResourceDictionary Source="CommonResources.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type vm:StartupViewModel}">
        <views:StartupView />
    </DataTemplate>
</ResourceDictionary>