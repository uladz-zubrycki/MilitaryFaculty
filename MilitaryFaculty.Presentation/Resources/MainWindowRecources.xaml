<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:MilitaryFaculty.Presentation.ViewModels"
                    xmlns:views="clr-namespace:MilitaryFaculty.Presentation.Views">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="CathedraResources.xaml"/>
        <ResourceDictionary Source="BookResources.xaml"/>
        <ResourceDictionary Source="ConferenceResources.xaml"/>
        <ResourceDictionary Source="FacultyTreeResources.xaml"/>
        <ResourceDictionary Source="ProfessorResources.xaml"/>
    </ResourceDictionary.MergedDictionaries>
    
    <DataTemplate DataType="{x:Type viewModels:StartupViewModel}">
        <views:StartupView/>
    </DataTemplate>
</ResourceDictionary>